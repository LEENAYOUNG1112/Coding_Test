//1
#include<iostream>
#include <cstring>
using namespace std;

int main()
{
	char* S = new char[1001];
	int n;

	cin >> S;
	cin >> n;

	int len = strlen(S);

	if (n <= len)
	{
		cout << S[n - 1];
	}

	delete[] S;
	S = nullptr;

	return 0;
}

//2
#include<iostream>
using namespace std;

int main()
{
	char S[1001];
	int n;

	cin >> S >> n;
	cout << S[n - 1];
    return 0;
}
