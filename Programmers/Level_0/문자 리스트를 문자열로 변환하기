#include <iostream>
#include <string>
#include <vector>

using namespace std;

string solution(vector<string> arr) {
	string answer = "";
// auto : 배열의 길이를 알아서 재주며 for문을 돌려준다
	for (auto i : arr) {
		answer += i;
	}
	return answer;
}

int main() {
	vector<string> arr = { "a", "b", "c" };
    solution(arr);

	return 0;
}
