#include<iostream>
using namespace std;

int main()
{
	const int a = 3;
	char* arr[a];

	for (int i = 0; i < a; i++)
	{
		arr[i] = new char[30];
		// 문자열 길이 동적 할당
		cin >> arr[i];
	}
	cout << endl;
	for (int i = 0; i < a; i++)
	{
		cout << arr[i] << endl;

	}
}

// 포인터 배열 : int* arr[3]; 
// 배열 포인터 : int (*arr)[3]; 
